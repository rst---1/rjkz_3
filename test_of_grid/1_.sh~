#!/bin/bash

# полный путь до скрипта
ABSOLUTE_FILENAME=`readlink -e "$0"`
# каталог в котором лежит скрипт
DIRECTORY=`dirname "$ABSOLUTE_FILENAME"`


"$DIRECTORY/Sections/2.py"
echo "2.py - done========================================="
"$DIRECTORY/Sections/3.sh"
echo "3.sh - done========================================="
"$DIRECTORY/Sections/4_tau_zx_x.py"
echo "4_tau_zx_x.py - done================================"
"$DIRECTORY/Sections/4_tau_zx_y.py"
echo "4_tau_zx_y.py - done================================"
"$DIRECTORY/Sections/4_tau_zy_x.py"
echo "4_tau_zy_x.py - done================================"
"$DIRECTORY/Sections/4_tau_zy_y.py"
echo "4_tau_zy_y.py - done================================"
"$DIRECTORY/Sections/5.sh"
echo "5.sh - done========================================="

